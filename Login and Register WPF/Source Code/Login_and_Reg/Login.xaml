<Window x:Class="Login_and_Reg.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login_and_Reg"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Button x:Name="but1" Content="Login" HorizontalAlignment="Left" Margin="506,336,0,0" VerticalAlignment="Top" Click="but1_Click"/>
        <Button x:Name="but2" Content="clear" HorizontalAlignment="Left" Margin="266,341,0,0" VerticalAlignment="Top" Click="but2_Click"/>
        <Label x:Name="Login1" Content="Login to profile" HorizontalAlignment="Center" Margin="0,77,0,0" VerticalAlignment="Top"/>
        <Label Content="Password :" HorizontalAlignment="Left" Margin="266,229,0,0" VerticalAlignment="Top"/>
        <Label Content="Email :" HorizontalAlignment="Left" Margin="266,159,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="box1" HorizontalAlignment="Left" Margin="419,163,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="box2" HorizontalAlignment="Left" Margin="419,233,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
